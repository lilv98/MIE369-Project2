(subactivity ?a1 ?a2)
(primitive ?a1 ?a2) 
(forall (?a1 ?a2)
	(implies  (subactivity ?a1 ?a2)
		  (and	(activity ?a1)
			(activity ?a2))))
(forall (?a)
	(implies  (activity ?a)
		  (subactivity ?a ?a)))
(forall (?a1 ?a2)
	(implies  (and	(subactivity ?a1 ?a2)
			(subactivity ?a2 ?a1))
		  (= ?a1 ?a2)))
(forall (?a1 ?a2 ?a3)
	(implies  (and	(subactivity ?a1 ?a2)
			(subactivity ?a2 ?a3))
		  (subactivity ?a1 ?a3)))
(forall (?a1 ?a2)
	(implies  (subactivity ?a1 ?a2)
		  (exists (?a3)
			(and	(subactivity ?a1 ?a3)
				(subactivity ?a3 ?a2)
				(forall (?a4)
					(implies  (and	(subactivity ?a1 ?a4)
							(subactivity ?a4 ?a3))
						  (or	(= ?a4 ?a1)
							(= ?a4 ?a3))))))))
(forall (?a1 ?a2)
	(implies  (subactivity ?a1 ?a2)
		  (exists (?a3)
			(and	(subactivity ?a1 ?a3)
				(subactivity ?a3 ?a2)
				(forall (?a4)
					(implies  (and	(subactivity ?a3 ?a4)
							(subactivity ?a4 ?a2))
						  (or	(= ?a4 ?a2)
							(= ?a4 ?a3))))))))
(forall (?a ?a1) (iff (primitive ?a)
                    (implies  (subactivity ?a1 ?a)
	                          (= ?a1 ?a))))
